<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zl.dto.domain.Son" table="SON" schema="SCOTT">
        <id name="id">
            <column name="ID" sql-type="varchar2(36)" length="36"/>
            <generator class="uuid"></generator>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar2(255)" not-null="true"/>
        </property>
        <property name="age">
            <column name="AGE" sql-type="number(*)" precision="2147483646" not-null="true"/>
        </property>
        <many-to-one name="fartherByParentId" class="com.zl.dto.domain.Farther" fetch="select">
            <column name="PARENT_ID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>